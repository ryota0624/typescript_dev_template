{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": [
            "es2015", "dom", "es2015.reflect"
        ],
        "typeRoots": [
            "./typings"
        ],
        "noImplicitAny": true,
        "alwaysStrict": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "strictNullChecks": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "jsx": "react",
        "outDir": "js"
    }, "include": [
        "./src/**/*",
        "typings/**/*"
    ]
}