{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": [
            "es2015", "dom"
        ],
        "noImplicitAny": true,
        "alwaysStrict": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "strictNullChecks": true,
        "sourceMap": true,
        "jsx": "react",
        "outDir": "js"
    }
}